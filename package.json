{
  "name": "react-babylon-playground",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tibotiber/react-babylon-playground"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "echo 'No tests'",
    "format": "prettier-eslint --write \"src/**/*.{js,json,css}\""
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css}": [
        "prettier-eslint --write",
        "git add"
      ]
    },
    "ignore": []
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "lint-staged": "^8.2.1",
    "prettier-eslint-cli": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
